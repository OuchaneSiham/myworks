generator client {
  provider = "prisma-client-js"
}
datasource db {
  provider = "postgresql"
}

model User {
  id       Int     @id @default(autoincrement())
  username String  @unique
  email    String  @unique
  password String?
  googleId String? @unique 
  avatar String @default("avatars/default.png")
  @@map("user")
}